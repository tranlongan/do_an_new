<!--set up phần up ảnh + ckeditor-->
<script>
    let file;
    document.querySelector("#file_image").onchange = (evt) => {
        file = evt.target.files[0];
        console.log(file);
    };

    <!--cài đặt ckeditor-->
    let editor;
    const initEditor = async () => {
        editor = await ClassicEditor
            .create(document.getElementById("ckeditor"), {
                cloudServices: {
                    uploadUrl: "https://76763.cke-cs.com/easyimage/upload/",
                    tokenUrl: "https://76763.cke-cs.com/token/dev/f0560f93197e924b7c2fe504e5b47712ccb702b4cefdf96d1d0ed0ca1be1"
                }
            });
    }

    initEditor();

    const btnDetailPost = async () => {
        const inputContent = document.getElementById("inputContent").value;
        const link_image = document.querySelector("#file_image");
        const detail_content = editor.getData();
        const inputMaterial = document.getElementById("inputMaterial").value;
        const inputMaterialPrice = document.getElementById("inputMaterialPrice").value;
        const inputTimeDo = document.getElementById("inputTimeDo").value;
        const selectLevelDo = document.getElementById("selectLevelDo").value;

        const formData = new FormData();
        formData.append("inputContent", inputContent);
        formData.append("image", link_image.files[0]);
        formData.append("detail_content",detail_content);
        formData.append("inputMaterial", inputMaterial);
        formData.append("inputMaterialPrice", inputMaterialPrice);
        formData.append("inputTimeDo", inputTimeDo);
        formData.append("selectLevelDo", selectLevelDo);

        try {
            const result = await axios.post(`/postDetail?id_user=<%= id_user %>`, formData, {
                headers: {
                    'Content-Type': 'multipart/form-data'
                }
            })
            const body = result.data;
            if (body.msg === 'post_success') {
                alert(`Bài viết của bạn đã được đăng tải`);
                window.open(`/user/homeUser?id_user=<%= id_user %>`, `_self`);
            }
        }catch (e) {
            console.error(e);
        }
    }
</script>
